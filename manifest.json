{
  "name": "GitHub issues storypoint",
  "version": "1",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/orgs/*/projects/*",
        "https://github.com/orgs/*/projects/*/views/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "icons": {
    "128": "icon_128.png"
  },
  "manifest_version": 3
}
